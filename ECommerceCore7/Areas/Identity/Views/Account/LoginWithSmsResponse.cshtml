@model LoginWithSmsResponseModel
@{
    ViewData["Title"] = "تایید ورود با کد پیامکی";
}


<div class="row">
    <div class="col-md-6 offset-md-3 col-sm-12 offset-sm-1" >
        <h1>@ViewData["Title"]</h1>
        <form method="post">
            <div asp-validation-summary="All" class="text-danger"></div>
            <input type="hidden" asp-for="AuthenticationKey"/>
            <div class="form-group">
                <label asp-for="AuthenticationCode"></label>
                <input asp-for="AuthenticationCode" class="form-control"/>
                <span asp-validation-for="AuthenticationCode" class="text-danger"></span>
            </div>
            <button type="submit" class="btn btn-primary">ورود</button>
        </form>
    </div>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
